#!/bin/bash

#Command line functions
help() {
	echo "yo"; 
}

build() {
 echo $flag $lab;
}


#Positional Parameters
flag=$1
lab=$2;

#Command line arguments
while getopts ":ht:b:" option; do
	case $option in
		h) help
		   exit;;
		#THM
		t) build $flag $lab 
			 exit;;
	    #HTB
		b) build $flag $lab
			 exit;;
		\?) echo "Invalid Option";
			help
			exit;;
	esac
done

